{
    "version": "1.2.0",
    "description": "Automate the creation of file structures using custom templates",
    "homepage": "https://gitlab.com/merrittcorp/fspop",
    "license": "",
    "architecture": {
        "64bit": {
            "url": "https://releases.merritt.es/fspop/v1.2.0/fspop-v1.2.0-windows_amd64.zip",
            "hash": "1c61440bbd8f4e7b31c3f90173bf3fd97c04621d958934bdff1c1a7951c34577"
        }
    },
    "bin": "fspop.exe",
    "checkver": {
		"url": "https://releases.merritt.es/fspop/",
        "regex": "(?<version>[0-9][\\.][0-9][\\.][0-9])",
		"reverse": true,
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://merritt.es/releases/fspop/v$version/fspop-v$version-windows_amd64.zip",
                "extract_dir": "fspop-v$version-win-64bit"
            }
        }
    }
}
