{
    "version": "1.3.1",
    "description": "Automate the creation of file structures using custom templates",
    "homepage": "https://gitlab.com/merrittcorp/fspop",
    "license": "",
    "architecture": {
        "64bit": {
            "url": "https://releases.merritt.es/fspop/v1.3.1/fspop-v1.3.1-windows_amd64.zip",
            "hash": "2e58ac6ae0c4b0590d5743324a2021e09b0bff295b4071aee38f66d6f2b9caf7"
        }
    },
    "bin": "fspop.exe",
    "checkver": {
		"url": "https://releases.merritt.es/fspop/",
        "regex": "(?<version>[0-9][\\.][0-9][\\.][0-9])",
		"reverse": true,
        "replace": "${version}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
